# MetaHuman Business Assistant - Environment Configuration
# Skopiuj ten plik jako .env i uzupe≈Çnij swoimi kluczami API

# Google AI Studio API Key (dla Gemini)
GOOGLE_API_KEY=your_google_ai_studio_key_here

# Notion API Key (dla workspace integration)
NOTION_API_KEY=your_notion_integration_key_here

# OpenAI API Key (opcjonalnie, dla dodatkowych funkcji)
OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud credentials (dla Calendar/Gmail - opcjonalnie)
GOOGLE_CLOUD_PROJECT=your_gcp_project_id
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# WebSocket Configuration (dla UE5)
WEBSOCKET_HOST=localhost
WEBSOCKET_PORT=8765

# Business Assistant Settings
ASSISTANT_NAME=MetaHuman Business Assistant
ASSISTANT_VOICE=male  # male/female
ASSISTANT_LANGUAGE=pl-PL

# Security Settings  
ENABLE_AUTHENTICATION=false
API_RATE_LIMIT=100  # requests per minute
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
